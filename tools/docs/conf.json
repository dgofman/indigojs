{
	"tags": {
		"allowUnknownTags" : true
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"fixMixin": true,
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "indigoJS"
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples"]
	}
}