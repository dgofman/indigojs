{
	"environment": "{{env}}",

	"server": {
		"port": {{port}},
		"webdir": "{{webdir}}",
		"cache": 3600 /* seconds, 0 - no-cache */
	},

	"locales": {
		"default": "en",
		"path": "{{locales}}",
		"monitor": 0 /* check file changes in seconds, 0 - no monitoring */
	},

	"routers": [
		"{{routers}}"
	],

	"modules": [
	]
}